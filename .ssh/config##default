# https://gitlab.com/-/snippets/2153849
Include config.d/*

Host *
  ServerAliveInterval=15
  ServerAliveCountMax=20
Host nas nas.home.pombei.ro
  HostName nas
  User admin
  RequestTTY yes
  IdentityAgent "~/Library/Group Containers/2BUA8C4S2C.com.1password/t/agent.sock"
  PreferredAuthentications publickey
Host unifi unifi.infra.pombei.ro gateway gateway.infra.pombei.ro 192.168.16.1
  User root
  HostKeyAlgorithms +ssh-rsa,ssh-dss
  PubkeyAcceptedKeyTypes=+ssh-rsa # Workaround since ssh-rsa is deprecated, until Ubiquiti starts accepting newer key types
  IdentityAgent "~/Library/Group Containers/2BUA8C4S2C.com.1password/t/agent.sock"
Host switch switch.infra.pombei.ro
  User pombeiro_admin
  IdentityAgent "~/Library/Group Containers/2BUA8C4S2C.com.1password/t/agent.sock"
Host ap ap.infra.pombei.ro ap-u6pro ap-u6pro.infra.pombei.ro
  HostName ap-u6pro
  User pombeiro_admin
  IdentityAgent "~/Library/Group Containers/2BUA8C4S2C.com.1password/t/agent.sock"
Host ernestoegraca.duckdns.org
  User root
  Port 10022

Host gitlab.com
  UseKeychain yes
  # Workaround UniFi Traffic inspector blocking SSH connections to GitLab
  # as ET SCAN Potential SSH Scan OUTBOUND: https://about.gitlab.com/blog/2016/02/18/gitlab-dot-com-now-supports-an-alternate-git-plus-ssh-port/
  Hostname altssh.gitlab.com
  User git
  Port 443
  IdentityAgent "~/Library/Group Containers/2BUA8C4S2C.com.1password/t/agent.sock"
  PreferredAuthentications publickey
Host github.com
  IdentityAgent "~/Library/Group Containers/2BUA8C4S2C.com.1password/t/agent.sock"
  PreferredAuthentications publickey

