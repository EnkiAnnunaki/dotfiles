# https://gitlab.com/-/snippets/2153849
Include config.d/*

Host *
    ServerAliveInterval=15
    ServerAliveCountMax=20
    IgnoreUnknown UseKeychain
Host nas nas.home.pombei.ro
    HostName nas
    User admin
    RemoteCommand cd /share/Container && exec sh --login
    RequestTTY yes
Host pihole pihole.dns.pombei.ro
    HostName pihole
    User pi
    RemoteCommand cd /etc && exec zsh --login
    RequestTTY yes
Host plexpi plexpi.home.pombei.ro
    HostName plexpi
    User root
Host gateway gateway.home.pombei.ro
    User pombeiro_admin
Host switch switch.home.pombei.ro
    User pombeiro_admin
Host ap ap.home.pombei.ro
    HostName ap-nanohd
    User pombeiro_admin

Host gitlab.com
    UseKeychain yes
Host github.com
    UseKeychain yes

